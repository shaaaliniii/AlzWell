<% if(success && success.length){ %>
    <div class="alert alert-success mt-3 alert-dismissible fade show"  ><%=success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>
<% if(error && error.length){ %>
    <div class="alert alert-danger mt-3 alert-dismissible fade show"  ><%=error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>